<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">

    <ItemGroup>
        <Content Include="$(MSBuildThisFileDirectory)$(Platform)\libcrunch.dll" Condition="Exists('$(MSBuildThisFileDirectory)$(Platform)\libcrunch.dll')">
			<Link>libcrunch.dll</Link>
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </Content>
	</ItemGroup>
	
	<ItemGroup>
		<Compile Include="$(MSBuildThisFileDirectory)..\..\src\CrunchPInvoke.cs">
			<Link>ManagedCrunch\CrunchPInvoke.cs</Link>
		</Compile>
	</ItemGroup>
	
</Project>